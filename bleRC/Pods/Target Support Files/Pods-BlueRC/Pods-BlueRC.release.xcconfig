GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/DGActivityIndicatorView" "${PODS_ROOT}/Headers/Public/TOMSMorphingLabel" "${PODS_ROOT}/Headers/Public/UICountingLabel"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/DGActivityIndicatorView" "$PODS_CONFIGURATION_BUILD_DIR/TOMSMorphingLabel" "$PODS_CONFIGURATION_BUILD_DIR/UICountingLabel"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/DGActivityIndicatorView" -isystem "${PODS_ROOT}/Headers/Public/TOMSMorphingLabel" -isystem "${PODS_ROOT}/Headers/Public/UICountingLabel"
OTHER_LDFLAGS = $(inherited) -ObjC -l"DGActivityIndicatorView" -l"TOMSMorphingLabel" -l"UICountingLabel" -framework "Foundation" -framework "UIKit"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
